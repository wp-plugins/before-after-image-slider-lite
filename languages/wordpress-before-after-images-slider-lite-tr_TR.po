#: vafpress/vafpress-field-definition.php:3
#: vafpress/vafpress-field-definition.php:4
msgid "Before After Image Slider"
msgstr "Öncesi - Sonrası Resim Slider"

#: vafpress/vafpress-field-definition.php:13
msgid "Before After Image Slider Lite"
msgstr "Öncesi - Sonrası Resim Slider Basit Sürüm"

#: vafpress/vafpress-field-definition.php:20
msgid "Title"
msgstr "Başlık"

#: vafpress/vafpress-field-definition.php:21
msgid "An optional title above the images."
msgstr "Görsel için isteğe bağlı başlık"

#: vafpress/vafpress-field-definition.php:25
msgid "Left Image"
msgstr "Sol Resim"

#: vafpress/vafpress-field-definition.php:28
msgid "The left image to use. (Required)"
msgstr "Kullanılacak sol görsel (Zorunlu)"

#: vafpress/vafpress-field-definition.php:32
msgid "Right Image"
msgstr "Sağ Resim"

#: vafpress/vafpress-field-definition.php:35
msgid "The right image to use. (Required)"
msgstr "Kullanılacak sağ görsel (Zorunlu)"

#: vafpress/vafpress-field-definition.php:39
msgid "Width"
msgstr "Genişlik"

#: vafpress/vafpress-field-definition.php:42
msgid "An optional width, use px or % (default is 500px)"
msgstr "İsteğe bağlı yenişlik, px ya da % kullanın (Varsayılan 500px)"

#: vafpress/vafpress-field-definition.php:46
msgid "Left Alt"
msgstr "Sol resim alt özelliği"

#: vafpress/vafpress-field-definition.php:48
msgid "Specify a value for the <b>\"alt\"</b> attribute of the left image."
msgstr "Sol resmin <b>\"alt\"</b> özelliği için bir değer girin"

#: vafpress/vafpress-field-definition.php:52
msgid "Right Alt"
msgstr "Sağ resim alt özelliği"

#: vafpress/vafpress-field-definition.php:54
msgid "Specify a value for the <b>\"alt\"</b> attribute of the right image."
msgstr "Sağ resmin <b>\"alt\"</b> özelliği için bir değer girin"

#: vafpress/vafpress-field-definition.php:58
msgid "CSS Classes (i.e. hover)"
msgstr "CSS Sınıfları"

#: vafpress/vafpress-field-definition.php:60
msgid "Additional CSS classes, use <b>\"hover\"</b> to enable these extras"
msgstr "Eklemek istediğiniz CSS'leri bu alana yazabilirsiniz"

#: vafpress/vafpress-field-definition.php:66
msgid "Want to support this plugin or need more features?"
msgstr "Bu eklentiyi desteklemek ister misin?"

